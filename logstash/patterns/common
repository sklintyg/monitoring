# ------------------------------------------------------------
# SESSION
# ------------------------------------------------------------
NO_SESSION NO SESSION
SESSION (?:%{NO_SESSION}|%{DATA})

# ------------------------------------------------------------
# WEBCERTTIMESTAMP
# ------------------------------------------------------------
WEBCERTDATE %{YEAR}[- ]%{MONTHNUM}[- ]%{MONTHDAY}
WEBCERTTIMESTAMP %{WEBCERTDATE}[ ]%{TIME}

# ------------------------------------------------------------
# UNIT
# ------------------------------------------------------------
NO_UNIT NO UNIT
UNIT (?:%{NO_UNIT}|%{DATA})

# ------------------------------------------------------------
# USER
# ------------------------------------------------------------
NO_USER NO USER
USER (?:%{NO_USER}|%{DATA})

# ------------------------------------------------------------
# LOG_LVL
# ------------------------------------------------------------
LOG_LVL (DEBUG|INFO|WARN|ERROR)

# ------------------------------------------------------------
# AUTH_PARSE
# ------------------------------------------------------------
USER_LOGIN Login user '%{DATA:user}' using scheme '%{DATA:scheme}'
USER_LOGOUT Logout user '%{DATA:user}' using scheme '%{DATA:scheme}'
USER_SESSION_EXPIRY Session expired for user '%{DATA:user}' using scheme '%{DATA:scheme}'
AUTH_PARSE (%{USER_LOGIN}|%{USER_LOGOUT}|%{USER_SESSION_EXPIRY})
